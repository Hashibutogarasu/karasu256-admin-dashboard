{
  "github": {
      "silent": true
  },
  "rewrites": [
    {
      "source": "/api/:path(.*)",
      "destination": "https://api.karasu256.com/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    },
    {
      "source": "/api-json",
      "destination": "https://api.karasu256.com/api-json"
    }
  ]
}